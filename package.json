{
  "name": "react-ld",
  "version": "0.2.1",
  "description": "Connects a Launch Darkly client with a react tree",
  "main": "lib/index.js",
  "repository": "git@github.com:Tabcorp/react-ld.git",
  "author": "Tabcorp",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "build:babel": "babel src --out-dir lib",
    "build:flow": "flow-copy-source --ignore '*.spec.*' src lib",
    "build": "yarn clean && yarn build:babel && yarn build:flow",
    "build:watch": "build:babel --watch",
    "prepublish": "yarn build"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-flow": "7.9.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^10.4.8",
    "babel-eslint": "10.1.0",
    "eslint": "7.0.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "flow-bin": "^0.152.0",
    "flow-copy-source": "2.0.9",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn flow"
    }
  },
  "lint-staged": {
    "**/*.js": "eslint"
  },
  "keywords": [
    "react",
    "launch-darkly"
  ]
}
